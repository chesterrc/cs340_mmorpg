{{!-- Create a table for inventory--}}
<table id="invts-tables">
    {{!-- Header section --}}
    <thead>
        <tr>
            {{#each data.[0]}}
            <th>
                {{@key}}
            </th>
            {{/each}}
        </tr>
    </thead>
    {{!-- Body section --}}
    <tbody>
        {{!-- For each row, print the id, fname, lname, homeworld and age, in order --}}
        {{#each data}}
        <tr>
            <td>{{this.inv_id}}</td>
            <td>{{this.inv_name}}</td>
            <td>{{this.items_item_id}}</td>
        </tr>
        {{/each}}
    </tbody>
</table>